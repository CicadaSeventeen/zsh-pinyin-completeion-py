from setuptools import setup
from Cython.Build import cythonize

setup(
    ext_modules=cythonize(
        "pypinyin",
        "unidecode",
        "anyascii",
        "distinguish_stringclass.py",
        "converter.py",
        "listdir.py",
        "completion.py",
        "listdir_conver.py",
        "main.py",
        compiler_directives={
            'language_level': 3,
            'boundscheck': False,
            'wraparound': False,
        }
    ),
)
